{
    "workbench.iconTheme": "vs-seti",
    "files.autoSave": "off",
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "npm.packageManager": "auto",
    "files.exclude": {
        // "**/.git": true,
        // "**/.svn": true,
        // "**/.hg": true,
        // "**/CVS": true,
        // "**/.DS_Store": true,
        // "**/tslint.json": true,
        // "**/tsfmt.json": true,
        // "**/tsconfig.json": true
    },
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": false,
    },
    "typescript.tsc.autoDetect": "off",
    "npm.autoDetect": "off",
    "yaml.customTags": [
        "!binary scalar",
        "!fragment scalar",
        "!function scalar",
    ],
    "yaml.schemas": {
        "/home/node/.config/yarn/global/node_modules/yaml-scene/schema.yas.json": "*.yas.yaml",
    }
}