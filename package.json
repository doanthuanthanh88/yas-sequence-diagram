{
  "name": "yas-sequence-diagram",
  "version": "1.0.0-beta.3",
  "description": "A Yaml-scene extension which generate sequence diagrams from comment line in code",
  "keywords": [
    "yaml-scene",
    "sequence-diagram",
    "extensions",
    "dev"
  ],
  "scripts": {
    "install:dist": "yarn install --production --modules-folder ./dist/node_modules",
    "dev": "npx nodemon --watch src --watch test --ext .ts --exec 'node -r ts-node/register -r ./bin/tsconfig-paths-bootstrap.js --inspect-brk src/index.ts'",
    "test": "npx jest --detectOpenHandles",
    "test:debug": "npx --node-options=\"--inspect-brk\" jest --detectOpenHandles",
    "doc": "npx yaml-scene scenes/export_guide.yaml",
    "prebuild": "yarn clean",
    "build": "tsc --project tsconfig.build.json",
    "postbuild": "node bin/tsconfig && node bin/update_package",
    "clean": "rm -rf dist",
    "prenewtag": "yarn test && yarn build",
    "newtag": "npx yaml-scene ./scenes/version.yaml",
    "postnewtag": "yarn clean"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.uniqwith": "^4.5.0"
  },
  "peerDependencies": {
    "yaml-scene": "^1.0.0-rc"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^14.14.6",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5",
    "yaml-scene": "^1.0.0-rc"
  },
  "author": {
    "name": "Đoàn Thuận Thành",
    "email": "doanthuanthanh88@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/doanthuanthanh88/yas-sequence-diagram"
  },
  "bugs": {
    "url": "https://github.com/doanthuanthanh88/yas-sequence-diagram/issues",
    "email": "doanthuanthanh88@gmail.com"
  }
}
